{
  "name": "cron-expression-input",
  "version": "1.3.1",
  "description": "Cron UI: input component to generate cron expressions easily and intuitively",
  "main": "dist/cron-expression-input.min.js",
  "files": [
    "dist/*",
    "lib/*",
    "lib/cultures/*",
    "src/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/cron-expression-input.js",
      "require": "./dist/cron-expression-input.umd.js",
      "types": "./lib/types/cron-expression-input.d.ts"
    },
    "./lib/cron-expression-input.min.js": {
      "import": "./dist/cron-expression-input.min.js",
      "require": "./dist/cron-expression-input.min.js"
    },
    "./lib/cron-expression-input.min.css": {
      "import": "./dist/cron-expression-input.min.css",
      "require": "./dist/cron-expression-input.min.css"
    },
    "./cultures/*": "./lib/cultures/*",
    "./src/*": "./src/*",
    "./style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KoushikDasika/cron-expression-input.git"
  },
  "author": "Jossy Devers <jossydevers@gmail.com> (https://github.com/JossyDevers)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JossyDevers/cron-expression-input/issues"
  },
  "homepage": "https://github.com/JossyDevers/cron-expression-input#readme",
  "keywords": [
    "cron",
    "crontab",
    "validate",
    "expression",
    "crontab guru",
    "ui",
    "editor",
    "generator",
    "unix"
  ],
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "cron-validator": "^1.2.1",
    "cronstrue": "^1.113.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "babel-loader": "^8.2.2",
    "vite": "^4.5.0",
    "typescript": "^5.0.0",
    "@types/node": "^18.0.0"
  },
  "types": "./lib/types/cron-expression-input.d.ts"
}
